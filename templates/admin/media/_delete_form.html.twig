<form method="post" action="{{ path('media_delete', {'id': medium.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer la vidéo ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ medium.id) }}">
    <button class="btn btn-danger mb-sm-2 mb-2 btn">Supprimer</button>
</form>
